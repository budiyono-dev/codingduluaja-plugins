{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/cda-block",
	"version": "0.1.0",
	"title": "Cda Header",
	"category": "text",
	"icon": "heading",
	"description": "Heading for list of content",
	"supports": {
		"html": false,
		"align": true,
		"color": {
			"background": true,
			"text": true
		},
		"typography": {
			"fontSize": true
		}
	},
	"attributes": {
        "text": {
            "type": "string"
        },
        "idHeader": {
            "type": "string"
        }
	},
    "example": {
        "attributes": {
            "text": "Judul",
            "idHeader": "paragrafPertama"
        }
    },
	"textdomain": "cda-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./style.css",
	"render": "file:./render.php"
}
